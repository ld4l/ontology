  <bf:Instance rdf:about="http://ld4l.library.cornell.edu/ld4l-vitro/individual/8019961instance21">
    <rdf:type rdf:resource="http://bibframe.org/vocab/Collection"/>
    <bf:instanceTitle rdf:resource="http://ld4l.library.cornell.edu/ld4l-vitro/individual/8019961title24"/>

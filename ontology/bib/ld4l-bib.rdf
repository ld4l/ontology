        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/format -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/format">
        <rdfs:label xml:lang="en-us">format</rdfs:label>
        <rdfs:comment xml:lang="en-us">File format or physical medium of an instance.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/formatOfMusic -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/formatOfMusic">
        <rdfs:label xml:lang="en-us">format of music</rdfs:label>
        <rdfs:comment xml:lang="en-us">Format of a musical composition, e.g. full score, condensed score, vocal score, etc.</rdfs:comment>
        <skos:editorialNote xml:lang="en-us">Need a vocabulary to enumerate values.</skos:editorialNote>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/geographicCoverageNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/geographicCoverageNote">
        <rdfs:label xml:lang="en-us">geographic coverage note</rdfs:label>
        <rdfs:comment xml:lang="en-us">Geographic entities covered by the resource.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/graphicScaleNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/graphicScaleNote">
        <rdfs:label xml:lang="en-us">graphic scale note</rdfs:label>
        <rdfs:comment xml:lang="en-us">Textual information about scale, including the scale of graphic material item such as architectural drawings or three-dimensional artifacts.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/languageNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/languageNote">
        <rdfs:label xml:lang="en-us">language note</rdfs:label>
        <rdfs:comment xml:lang="en-us">Note concerning the language of the material or its parts.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/legalDate -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/legalDate">
        <rdfs:label xml:lang="en-us">legal date</rdfs:label>
        <rdfs:comment xml:lang="en-us">Date of legal work, or promulgation of a law, or signing of a treaty.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/modeOfIssuance -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/modeOfIssuance">
        <rdfs:label xml:lang="en-us">mode of issuance</rdfs:label>
        <rdfs:comment xml:lang="en-us">Categorization reflecting whether a resource is issued in one or more parts, the way it is updated, and its intended termination.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/musicMediumNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/musicMediumNote">
        <rdfs:label xml:lang="en-us">music medium note</rdfs:label>
        <rdfs:comment xml:lang="en-us">Instrumental, vocal, and/or other medium of performance for which a musical resource was originally conceived, written or performed.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/notationSystem -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/notationSystem">
        <rdfs:label xml:lang="en-us">notation system</rdfs:label>
        <rdfs:comment xml:lang="en-us">Information on the alphabet, script, or symbol system used to convey the content of the resource, including specialized scripts, typefaces, tactile notation, and musical notation.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/performerNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/performerNote">
        <rdfs:label xml:lang="en-us">performer note</rdfs:label>
        <rdfs:comment xml:lang="en-us">Information about the participants, players, narrators, presenters, or performers.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/providerStatement -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/providerStatement">
        <rdfs:label xml:lang="en-us">provider statement</rdfs:label>
        <rdfs:comment xml:lang="en-us">Transcribed provider statement</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/supplementaryContentNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/supplementaryContentNote">
        <rdfs:label xml:lang="en-us">supplementary content note</rdfs:label>
        <rdfs:comment xml:lang="en-us">Information on the presence of one or more bibliographies, discographies, filmographies, and/or other bibliographic references in a described resource or in accompanying material.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/temporalCoverageNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/temporalCoverageNote">
        <rdfs:label xml:lang="en-us">temporal coverage note</rdfs:label>
        <rdfs:comment xml:lang="en-us">Time period covered by the resource.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/treatySignator -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/treatySignator">
        <rdfs:label xml:lang="en-us">treaty signatory</rdfs:label>
        <rdfs:comment xml:lang="en-us">Government or other party that has formally signed a treaty.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/elements/1.1/date -->

    <owl:DatatypeProperty rdf:about="&dc;date">
        <rdfs:label xml:lang="en-us">date</rdfs:label>
        <rdfs:comment xml:lang="en-us">A point or period of time associated with an event in the lifecycle of the resource.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://purl.org/dc/terms/created -->

    <owl:DatatypeProperty rdf:about="&dcterms;created">
        <rdfs:label xml:lang="en-us">created</rdfs:label>
        <rdfs:comment xml:lang="en-us">Date of creation of the resource.</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://schema.org/accessibilityFeature -->

    <owl:DatatypeProperty rdf:about="http://schema.org/accessibilityFeature">
        <rdfs:label xml:lang="en-us">accessibility feature</rdfs:label>
        <rdfs:comment xml:lang="en-us">Accessibility feature of the resource, such as accessible media, alternatives and supported enhancements for accessibility (WebSchemas wiki lists possible values).</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://schema.org/birthDate -->

    <owl:DatatypeProperty rdf:about="http://schema.org/birthDate"/>
    


    <!-- http://schema.org/deathDate -->

    <owl:DatatypeProperty rdf:about="http://schema.org/deathDate"/>
    


    <!-- http://www.w3.org/2011/content#chars -->

    <owl:DatatypeProperty rdf:about="&cnt;chars">
        <rdfs:label xml:lang="en-us">chars</rdfs:label>
        <rdfs:comment xml:lang="en-us">The character sequence of the text content.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain rdf:resource="&cnt;ContentAsText"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/oa#annotatedAt -->

    <owl:DatatypeProperty rdf:about="&oa;annotatedAt">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en-us">annotated at</rdfs:label>
        <rdfs:comment xml:lang="en-us">The time at which the Annotation was created.</rdfs:comment>
        <rdfs:range rdf:resource="&xsd;dateTimeStamp"/>
        <rdfs:domain rdf:resource="&oa;Annotation"/>
    </owl:DatatypeProperty>
    


    <!-- http://www.w3.org/ns/prov#startedAtTime -->

    <owl:DatatypeProperty rdf:about="&prov;startedAtTime">
        <rdfs:label xml:lang="en-us">started at time</rdfs:label>
        <rdfs:comment xml:lang="en-us">The time at which an activity started. See also prov:endedAtTime.</rdfs:comment>

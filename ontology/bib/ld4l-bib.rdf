        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/duration -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/duration">
        <rdfs:label xml:lang="en-us">duration</rdfs:label>
        <rdfs:comment xml:lang="en-us">Information about the playing time or duration in an unstructured form, e.g., &quot;2 hours&quot;.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/findingAidNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/findingAidNote">
        <rdfs:label xml:lang="en-us">finding aid note</rdfs:label>
        <rdfs:comment xml:lang="en-us">Note about availability of an index or finding aid.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/formDesignation -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/formDesignation">
        <rdfs:label xml:lang="en-us">form designation</rdfs:label>

        <rdfs:comment xml:lang="en-us">Information on awards associated with the described resource.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/colorContent -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/colorContent">
        <rdfs:label xml:lang="en-us">color content</rdfs:label>
        <rdfs:comment xml:lang="en-us">Color characteristics, e.g. black and white, multicolored, etc.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/contentsNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/contentsNote">
        <rdfs:label xml:lang="en-us">contents note</rdfs:label>
        <rdfs:comment xml:lang="en-us">List of subunits of the resource.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/creditsNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/creditsNote">
        <rdfs:label xml:lang="en-us">credits note</rdfs:label>

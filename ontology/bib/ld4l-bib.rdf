<?xml version="1.0"?>


<!DOCTYPE rdf:RDF [
    <!ENTITY oa "http://www.w3.org/ns/oa#" >
    <!ENTITY dcterms "http://purl.org/dc/terms/" >
    <!ENTITY prov "http://www.w3.org/ns/prov#" >
    <!ENTITY foaf "http://xmlns.com/foaf/0.1/" >
    <!ENTITY vann "http://purl.org/vocab/vann/" >
    <!ENTITY fabio "http://purl.org/spar/fabio/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY madsrdf "http://www.loc.gov/mads/rdf/v1#" >
    <!ENTITY cnt "http://www.w3.org/2011/content#" >
    <!ENTITY dc "http://purl.org/dc/elements/1.1/" >
    <!ENTITY vivo "http://vivoweb.org/ontology/core#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdau "http://rdaregistry.info/Elements/u/" >
    <!ENTITY skos "http://www.w3.org/2004/02/skos/core#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY iana "http://www.iana.org/assignments/relation/" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY vs "http://www.w3.org/2003/06/sw-vocab-status/ns#" >
]>


<rdf:RDF xmlns="http://ld4l.org/ontology/bib/"
     xml:base="http://ld4l.org/ontology/bib/"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:prov="http://www.w3.org/ns/prov#"
     xmlns:foaf="http://xmlns.com/foaf/0.1/"
     xmlns:cnt="http://www.w3.org/2011/content#"
     xmlns:madsrdf="http://www.loc.gov/mads/rdf/v1#"
     xmlns:vann="http://purl.org/vocab/vann/"
     xmlns:iana="http://www.iana.org/assignments/relation/"
     xmlns:fabio="http://purl.org/spar/fabio/"
     xmlns:vs="http://www.w3.org/2003/06/sw-vocab-status/ns#"
     xmlns:dcterms="http://purl.org/dc/terms/"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:oa="http://www.w3.org/ns/oa#"
     xmlns:rdau="http://rdaregistry.info/Elements/u/"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:vivo="http://vivoweb.org/ontology/core#">
    <owl:Ontology rdf:about="http://ld4l.org/ontology/bib/">
        <rdfs:label xml:lang="en-us">LD4L Proposed Bibliographic Ontology</rdfs:label>
        <dcterms:modified>2015-07-23</dcterms:modified>
        <dcterms:issued>2015-07-23</dcterms:issued>
        <rdfs:comment xml:lang="en-us">This bibliographic ontology is based on Rob Sanderson&apos;s and LD4L&apos;s recommended changes to BIBFRAME to better reflect best practices in the Linked Data domain. It is LD4Lâ€™s hope that derivations from BIBFRAME found in this ontology will be folded into the official BIBFRAME namespace. Efforts have been made to consider each class and property, but this ontology is largely untested. LD4L may provide revised/expanded versions in future as we identify new use cases and begin to test the ontology with instance data. The proposed changes include, but are not limited to, incorporation of the following conventions: (1) Reuse stable pre-existing classes and properties from external ontologies rather than declaring new ones within the BIBFRAME namespace. (2) Use URIs rather than strings to identify resources. (3) Replace the bf:Authority classes with Real World Entity classes for people, places, things, etc. (4) Define only one pattern to model one feature of the knowledge domain. (5) Clarify the directionality of properties via naming, definitions, and, where applicable, domain and range constraints, and add inverse properties where appropriate. (6) Name terms consistently and make the distinction between classes, object properties, and datatype properties clear through standard naming conventions.</rdfs:comment>
        <owl:versionInfo xml:lang="en-us">Version 1.0</owl:versionInfo>
        <vann:preferredNamespacePrefix>ld4l</vann:preferredNamespacePrefix>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Annotation properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://ld4l.org/ontology/bib/note -->

    <owl:AnnotationProperty rdf:about="http://ld4l.org/ontology/bib/note"/>
    


    <!-- http://purl.org/vocab/vann/preferredNamespacePrefix -->

    <owl:AnnotationProperty rdf:about="&vann;preferredNamespacePrefix">
        <rdfs:label xml:lang="en">preferred namespace prefix</rdfs:label>
        <rdfs:comment xml:lang="en">The preferred namespace prefix to use when using terms from this vocabulary.</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://purl.org/vocab/vann/"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2000/01/rdf-schema#range -->

    <owl:AnnotationProperty rdf:about="&rdfs;range"/>
    


    <!-- http://www.w3.org/2003/06/sw-vocab-status/ns#term_status -->

    <owl:AnnotationProperty rdf:about="&vs;term_status">
        <rdfs:label>term status</rdfs:label>
        <vs:term_status>unstable</vs:term_status>
        <rdfs:comment>The status of a vocabulary term, expressed as a short symbolic string; known values include &apos;unstable&apos;,&apos;testing&apos;, &apos;stable&apos; and &apos;archaic&apos;</rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2003/06/sw-vocab-status/ns#"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#definition -->

    <owl:AnnotationProperty rdf:about="&skos;definition">
        <rdfs:label xml:lang="en">definition</rdfs:label>
        <skos:definition xml:lang="en">A statement or formal explanation of the meaning of a concept.</skos:definition>
        <rdfs:subPropertyOf rdf:resource="&vocab;note"/>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#editorialNote -->

    <owl:AnnotationProperty rdf:about="&skos;editorialNote">
        <rdfs:label xml:lang="en">editorial note</rdfs:label>
        <skos:definition xml:lang="en">A note for an editor, translator or maintainer of the vocabulary.</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <rdfs:subPropertyOf rdf:resource="&skos;note"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#example -->

    <owl:AnnotationProperty rdf:about="&skos;example">
        <rdfs:label xml:lang="en">example</rdfs:label>
        <skos:definition xml:lang="en">An example of the use of a concept.</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <rdfs:subPropertyOf rdf:resource="&skos;note"/>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#note -->

    <owl:AnnotationProperty rdf:about="&skos;note">
        <rdfs:label xml:lang="en-us">note</rdfs:label>
        <skos:definition xml:lang="en-us">A general note, for any purpose.</skos:definition>
    </owl:AnnotationProperty>
    


    <!-- http://www.w3.org/2004/02/skos/core#scopeNote -->

    <owl:AnnotationProperty rdf:about="&skos;scopeNote">
        <rdfs:label xml:lang="en">scope note</rdfs:label>
        <skos:definition xml:lang="en">A note that helps to clarify the meaning and/or the use of a concept.</skos:definition>
        <rdfs:isDefinedBy rdf:resource="http://www.w3.org/2004/02/skos/core"/>
        <rdfs:subPropertyOf rdf:resource="&skos;note"/>
    </owl:AnnotationProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Datatypes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- &amp;xsd;boolean -->

    <rdfs:Datatype rdf:about="&amp;xsd;boolean"/>
    


    <!-- http://ld4l.org/ontology/bib/&amp;xsd;boolean -->

    <rdfs:Datatype rdf:about="http://ld4l.org/ontology/bib/&amp;xsd;boolean"/>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://ld4l.org/ontology/bib/absorbedBy -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/absorbedBy">
        <rdfs:label xml:lang="en-us">absorbed by</rdfs:label>
        <rdfs:comment xml:lang="en-us">This Work has been incorporated into another Work.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/precedes"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/accompaniedBy -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/accompaniedBy">
        <rdfs:label xml:lang="en-us">accompanied by</rdfs:label>
        <skos:example xml:lang="en-us">An encyclopedia is accompanied by an index in a separate volume.</skos:example>
        <rdfs:comment xml:lang="en-us">This resource is accompanied by another resource that adds to it.</rdfs:comment>
        <owl:inverseOf rdf:resource="http://ld4l.org/ontology/bib/accompanies"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/accompanies -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/accompanies">
        <rdfs:label xml:lang="en-us">accompanies</rdfs:label>
        <skos:example xml:lang="en-us">An index in a separate volume accompanies an encyclopedia.</skos:example>
        <rdfs:comment xml:lang="en-us">This resource adds to or is issued with another resource.</rdfs:comment>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/classifiedAs -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/classifiedAs">
        <rdfs:label xml:lang="en-us">classified as</rdfs:label>
        <rdfs:comment xml:lang="en-us">Classification of this resource in some scheme.</rdfs:comment>
        <skos:scopeNote xml:lang="en-us">The skos:Concept that is the object of this property would be part of a skos:ConceptScheme.</skos:scopeNote>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Classification"/>
        <owl:inverseOf rdf:resource="http://ld4l.org/ontology/bib/isClassificationFor"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/continuedUnderNewTitleBy -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/continuedUnderNewTitleBy">
        <rdfs:label xml:lang="en-us">continued under new title by</rdfs:label>
        <rdfs:comment xml:lang="en-us">The content of this Work is continued by a later work under a new title.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <owl:inverseOf rdf:resource="http://ld4l.org/ontology/bib/continuesUnderNewTitle"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/precedes"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/continuesUnderNewTitle -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/continuesUnderNewTitle">
        <rdfs:label xml:lang="en-us">continues under new title</rdfs:label>
        <rdfs:comment xml:lang="en-us">This Work continues the content of an earlier Work under a new title.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/succeeds"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/createdForDegree -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/createdForDegree">
        <rdfs:label xml:lang="en-us">dissertation degree</rdfs:label>
        <rdfs:comment xml:lang="en-us">This work was created in partial fulfillment of the requirements of the specified degree.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:range rdf:resource="&vivo;AcademicDegree"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/derivedFrom -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/derivedFrom">
        <rdfs:label xml:lang="en-us">derived from</rdfs:label>
        <skos:example xml:lang="en-us">A translated Title is derived from an original Title. A translated Work is derived from an original Work.</skos:example>
        <rdfs:comment xml:lang="en-us">This resource was derived from another resource.</rdfs:comment>
        <skos:scopeNote xml:lang="en-us">Titles can be derived from other Titles, and Works can be derived from other Works. Property restrictions on Work and Title prevent linking subjects and objects of different types.</skos:scopeNote>
        <owl:inverseOf rdf:resource="http://ld4l.org/ontology/bib/hasDerivative"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/succeeds"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ld4l.org/ontology/bib/Work"/>
                    <rdf:Description rdf:about="&madsrdf;Title"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ld4l.org/ontology/bib/Work"/>
                    <rdf:Description rdf:about="&madsrdf;Title"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/distributedBy -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/distributedBy">
        <rdfs:label xml:lang="en-us">distributed by</rdfs:label>
        <skos:scopeNote xml:lang="en-us">The distribution Activity is related to an Agent that performed or performs the distribution via prov:wasAssociatedWith.</skos:scopeNote>
        <rdfs:comment xml:lang="en-us">This Instance was or is distributed by the specified Activity.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/providedBy"/>
        <rdfs:range rdf:resource="&prov;Activity"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/equivalentTo -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/equivalentTo">
        <rdf:type rdf:resource="&owl;SymmetricProperty"/>
        <rdfs:label xml:lang="en-us">equivalent to</rdfs:label>
        <rdfs:comment xml:lang="en-us">This Instance embodies the same expression of a work as the specified Instance.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/formedBySplitting -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/formedBySplitting">
        <rdfs:label xml:lang="en-us">formed by splitting</rdfs:label>
        <rdfs:comment xml:lang="en-us">The subject works were formed by splitting another work.</rdfs:comment>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <owl:inverseOf rdf:resource="http://ld4l.org/ontology/bib/splitInto"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/succeeds"/>
        <rdfs:domain>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dcterms;hasPart"/>
                        <owl:allValuesFrom rdf:resource="http://ld4l.org/ontology/bib/Work"/>
                    </owl:Restriction>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="&dcterms;hasPart"/>
                        <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">2</owl:minCardinality>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/governedBy -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/governedBy">
        <rdfs:label xml:lang="en-us">governed by</rdfs:label>
        <rdfs:comment xml:lang="en-us">This Item is governed by the specified PolicySet.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Item"/>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/PolicySet"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/governs -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/governs">
        <rdfs:label xml:lang="en-us">governeds</rdfs:label>
        <rdfs:comment xml:lang="en-us">This PolicySet governs the Item.</rdfs:comment>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Item"/>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/PolicySet"/>
        <owl:inverseOf rdf:resource="http://ld4l.org/ontology/bib/governedBy"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/hasAbsorbed -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/hasAbsorbed">
        <rdfs:label xml:lang="en-us">has absorbed</rdfs:label>
        <rdfs:comment xml:lang="en-us">This Work has absorbed or incorporated another Work.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <owl:inverseOf rdf:resource="http://ld4l.org/ontology/bib/absorbedBy"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/succeeds"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/hasAccessPolicy -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/hasAccessPolicy">
        <rdfs:label xml:lang="en-us">has access policy</rdfs:label>
        <skos:scopeNote>Items (holdings) are related to this PolicySet via ld4l:governedBy.</skos:scopeNote>
        <rdfs:comment xml:lang="en-us">Policy statement belonging to this PolicySet specifying allowances and restrictions on access to the related holdings.</rdfs:comment>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Policy"/>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/PolicySet"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/hasPolicy"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/hasAnnotation -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/hasAnnotation">
        <rdfs:label xml:lang="en-us">has annotation</rdfs:label>
        <rdfs:comment xml:lang="en-us">Annotation of this resource.</rdfs:comment>
        <rdfs:range rdf:resource="&oa;Annotation"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/hasCirculationStatus -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/hasCirculationStatus">
        <rdfs:label xml:lang="en-us">has circulation status</rdfs:label>
        <skos:editorialNote xml:lang="en-us">Might be a FunctionalProperty, but possibly we want to track circulation status over time.</skos:editorialNote>
        <rdfs:comment xml:lang="en-us">The circulation status of this item.</rdfs:comment>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/CirculationStatus"/>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Item"/>
        <owl:inverseOf rdf:resource="http://ld4l.org/ontology/bib/isCirculationStatusFor"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/hasContribution -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/hasContribution">
        <rdfs:label xml:lang="en">has Contribution</rdfs:label>
        <rdfs:comment xml:lang="en">This work was contributed to through this contribution.</rdfs:comment>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Contribution"/>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/hasCoverArt -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/hasCoverArt">
        <rdfs:label xml:lang="en-us">has cover art</rdfs:label>
        <rdfs:comment xml:lang="en-us">Cover art for this Instance.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/hasDataSource -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/hasDataSource">
        <rdfs:label xml:lang="en-us">has data source</rdfs:label>
        <rdfs:comment xml:lang="en-us">Work that is a data source for this resource. It may contain information about other files, printed sources, or collection procedures.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Work"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/hasDerivative -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/hasDerivative">
        <rdfs:label xml:lang="en-us">has derivative</rdfs:label>
        <skos:scopeNote xml:lang="en-us">The domain and range restrictions ensure that a Work is related to a Work and a Title to a Title.</skos:scopeNote>
        <rdfs:comment xml:lang="en-us">Work or Title that is derived from this Work or Title.</rdfs:comment>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/precedes"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ld4l.org/ontology/bib/Work"/>
                    <rdf:Description rdf:about="&madsrdf;Title"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
        <rdfs:range>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ld4l.org/ontology/bib/Work"/>
                    <rdf:Description rdf:about="&madsrdf;Title"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:range>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/hasExpression -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/hasExpression">
        <rdfs:label xml:lang="en-us">has expression</rdfs:label>
        <rdfs:comment xml:lang="en-us">This Work has a related expression. For use to connect Works under FRBR/RDA rules.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <owl:inverseOf rdf:resource="http://ld4l.org/ontology/bib/isExpressionOf"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/precedes"/>
    </owl:ObjectProperty>
    


    <!-- http://ld4l.org/ontology/bib/hasFindingAid -->

    <owl:ObjectProperty rdf:about="http://ld4l.org/ontology/bib/hasFindingAid">
        <rdfs:label xml:lang="en-us">has finding aid</rdfs:label>
        <rdfs:comment xml:lang="en-us">This Work has a finding aid or similar control material for archival, visual, and manuscript resources.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:range rdf:resource="http://ld4l.org/ontology/bib/Work"/>

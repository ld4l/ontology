        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/legacy/awardNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/legacy/awardNote">

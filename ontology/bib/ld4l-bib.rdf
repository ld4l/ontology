    //
    // Data properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://ld4l.org/ontology/bib/acquisitionNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/acquisitionNote">
        <rdfs:label xml:lang="en-us">acquisition note</rdfs:label>
        <rdfs:comment xml:lang="en-us">Information about the circumstances (e.g., source, date, method) under which the resource was directly acquired.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Item"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/barcode -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/barcode">
        <rdfs:label xml:lang="en-us">barcode</rdfs:label>
        <rdfs:comment xml:lang="en-us">Identification number of the physical item.</rdfs:comment>
        <skos:editorialNote xml:lang="en-us">Probably not a FunctionalProperty: there are both Codabar and Code 39 barcoding systems; could an Item have both? </skos:editorialNote>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Item"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/cartographicEasternmostLongitude -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/cartographicEasternmostLongitude">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en-us">cartographic easternmost longitude</rdfs:label>
        <rdfs:comment xml:lang="en-us">Records easternmost longitudinal coordinate of the geographic entity. The abbreviations for the hemispheres are: N = North, S = South, E = East, W = West. The coordinate may be recorded in the form hdddmmss (hemisphere-degrees-minutes-seconds); however, other forms are also allowed, such as decimal degrees. Unused positions contain zeros.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Cartography"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/cartographicEquinox -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/cartographicEquinox">
        <rdfs:label xml:lang="en-us">cartographic equinox</rdfs:label>
        <rdfs:comment xml:lang="en-us">Statement of equinox or epoch.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Cartography"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/cartographicExclusionGRingCoordinates -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/cartographicExclusionGRingCoordinates">
        <rdfs:label xml:lang="en-us">cartographic exclusion G ring coordinates</rdfs:label>
        <skos:editorialNote xml:lang="en-us">Break the pair up into its individual elements? What would they be named?</skos:editorialNote>
        <rdfs:comment xml:lang="en-us">Coordinate pair that identifies the closed non-intersecting boundary of the area contained within the G-polygon outer ring that is excluded.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Cartography"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/cartographicNorthernmostLatitude -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/cartographicNorthernmostLatitude">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en-us">cartographic northernmost latitude</rdfs:label>
        <rdfs:comment xml:lang="en-us">Records northernmost latitudinal coordinate of the geographic entity. The abbreviations for the hemispheres are: N = North, S = South, E = East, W = West. The coordinate may be recorded in the form hdddmmss (hemisphere-degrees-minutes-seconds); however, other forms are also allowed, such as decimal degrees. Unused positions contain zeros.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Cartography"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/cartographicOuterGRingCoordinates -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/cartographicOuterGRingCoordinates">
        <rdfs:label xml:lang="en-us">cartographic outer G ring coordinates</rdfs:label>
        <skos:editorialNote xml:lang="en-us">Break the pair up into its individual elements? What would they be named?</skos:editorialNote>
        <rdfs:comment xml:lang="en-us">Coordinate pair that identifies the closed non-intersecting boundary of the area covered.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Cartography"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/cartographicProjection -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/cartographicProjection">
        <rdfs:label xml:lang="en-us">cartographic projection</rdfs:label>
        <rdfs:comment xml:lang="en-us">Method or system used to represent the surface of the Earth or of a celestial sphere on a plane.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Cartography"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/cartographicScale -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/cartographicScale">
        <rdfs:label xml:lang="en-us">cartographic scale</rdfs:label>
        <rdfs:comment xml:lang="en-us">Ratio of the dimensions of a form contained or embodied in a resource to the dimensions of the entity it represents. Entire scale statement including any equivalency statements, vertical scales or vertical exaggeration statements for relief models and other three-dimensional items.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Cartography"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/cartographicSouthernmostLatitude -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/cartographicSouthernmostLatitude">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en-us">cartographic southernmost latitude</rdfs:label>
        <rdfs:comment xml:lang="en-us">Records southernmost latitudinal coordinate of the geographic entity. The abbreviations for the hemispheres are: N = North, S = South, E = East, W = West. The coordinate may be recorded in the form hdddmmss (hemisphere-degrees-minutes-seconds); however, other forms are also allowed, such as decimal degrees. Unused positions contain zeros.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Cartography"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/cartographicWesternmostLongitude -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/cartographicWesternmostLongitude">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en-us">cartographic westernmost longitude</rdfs:label>
        <rdfs:comment xml:lang="en-us">Records westernmost longitudinal coordinate of the geographic entity. The abbreviations for the hemispheres are: N = North, S = South, E = East, W = West. The coordinate may be recorded in the form hdddmmss (hemisphere-degrees-minutes-seconds); however, other forms are also allowed, such as decimal degrees. Unused positions contain zeros.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Cartography"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/cartographicZone -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/cartographicZone">
        <rdf:type rdf:resource="&owl;FunctionalProperty"/>
        <rdfs:label xml:lang="en-us">cartographic zone</rdfs:label>
        <rdfs:comment xml:lang="en-us">Cartographic zone used for celestial charts.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Cartography"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/copyrightDate -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/copyrightDate">
        <rdfs:label xml:lang="en-us">copyright date</rdfs:label>
        <rdfs:comment xml:lang="en-us">Date associated with a claim of protection under copyright or a similar regime.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/custodialHistory -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/custodialHistory">
        <rdfs:label xml:lang="en-us">custodial history</rdfs:label>
        <rdfs:comment xml:lang="en-us">Information about the provenance, such as origin, ownership and custodial history (chain of custody), of a resource.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/dimensions -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/dimensions">
        <rdfs:label xml:lang="en-us">dimensions</rdfs:label>
        <rdfs:comment xml:lang="en-us">Measurements of the carrier or carriers and/or the container of a resource.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:domain>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://ld4l.org/ontology/bib/Instance"/>
                    <rdf:Description rdf:about="http://ld4l.org/ontology/bib/Item"/>
                </owl:unionOf>
            </owl:Class>
        </rdfs:domain>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/dissertationNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/dissertationNote">
        <rdfs:label xml:lang="en-us">dissertation note</rdfs:label>
        <skos:editorialNote xml:lang="en-us">Include dissertation note as a literal when it cannot be parsed into dissertationYear, dissertationDegree, etc.</skos:editorialNote>
        <rdfs:comment xml:lang="en-us">Textual information about the dissertation. A dissertation note differs from a general note in that it includes specific date and year information.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Dissertation"/>
        <rdfs:subPropertyOf rdf:resource="http://ld4l.org/ontology/bib/note"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/dissertationYear -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/dissertationYear">
        <rdfs:label xml:lang="en-us">dissertation year</rdfs:label>
        <rdfs:comment xml:lang="en-us">Year degree awarded.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Dissertation"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/extent -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/extent">
        <rdfs:label xml:lang="en-us">extent</rdfs:label>
        <rdfs:comment xml:lang="en-us">Number and type of units and/or subunits making up a resource.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/frequency -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/frequency">
        <rdfs:label xml:lang="en-us">frequency</rdfs:label>
        <skos:scopeNote>Frequency refers to intended rather than actual frequency, so that a Serial that rarelly meets deadlines is still a Periodical, and a Series that appears regularly but does not promise to do so does not thereby turn into a Periodical.</skos:scopeNote>
        <rdfs:comment xml:lang="en-us">Intervals at which the issues or parts of a serial or the updates to an integrating resource are issued.</rdfs:comment>
        <skos:editorialNote xml:lang="en-us">Need to locate an appropriate time interval vocabulary to create NamedIndividuals from. Then this would change  from a DatatypeProperty to an ObjectProperty.</skos:editorialNote>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/generationDate -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/generationDate">
        <rdfs:label xml:lang="en-us">generation date</rdfs:label>
        <rdfs:comment xml:lang="en-us">Date of conversion of the data from another format.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/hasIndex -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/hasIndex">
        <rdfs:label xml:lang="en-us">has index</rdfs:label>
        <rdfs:range rdf:datatype="&amp;xsd;boolean"></rdfs:range>
        <rdfs:comment xml:lang="en-us">Indicates whether a work or instance contains an internal index</rdfs:comment>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/illustrationNote -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/illustrationNote">
        <rdfs:label xml:lang="en-us">illustration note</rdfs:label>
        <rdfs:comment xml:lang="en-us">Information about illustrative material in the resource.</rdfs:comment>
        <rdfs:domain rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/musicNumber -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/musicNumber">
        <rdfs:label xml:lang="en-us">music number</rdfs:label>
        <rdfs:comment xml:lang="en-us">Serial, opus, or thematic number or code for music.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    


    <!-- http://ld4l.org/ontology/bib/note -->

    <owl:DatatypeProperty rdf:about="http://ld4l.org/ontology/bib/note">

    <!-- http://ld4l.org/ontology/bib/AbbreviatedTitle -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/AbbreviatedTitle">
        <rdfs:label xml:lang="en-us">Abbreviated title</rdfs:label>
        <rdfs:subClassOf rdf:resource="&madsrdf;Title"/>
        <rdfs:comment xml:lang="en-us">Title as abbreviated for indexing or identification.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Ansi -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Ansi">
        <rdfs:label xml:lang="en-us">ANSI</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">American National Standards Institute identifier.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Audio -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Audio">
        <rdfs:label xml:lang="en-us">Audio</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:comment xml:lang="en-us">Resources expressed in an audible form, including music or other sounds.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Award -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Award">
        <rdfs:label xml:lang="en-us">Award</rdfs:label>
        <rdfs:comment xml:lang="en-us">An award or honor bestowed upon an Agent or Work. The relationship between Award and recipient is mediated by an AwardReceipt, which may contain additional information, such as date.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/AwardReceipt -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/AwardReceipt">
        <rdfs:label xml:lang="en-us">Award receipt</rdfs:label>
        <rdfs:comment xml:lang="en-us">The bestowal of an award, honor, or distinction to an Agent or Work. The award bestowed should be represented with the Award class. The AwardReceipt is a context node between the award and the recipient which may contain further information, such as date.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/BinderTitle -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/BinderTitle">
        <rdfs:label>Binder title</rdfs:label>
        <rdfs:subClassOf rdf:resource="&madsrdf;Title"/>
        <rdfs:comment>Title printed on an item&apos;s cover after rebinding.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/CaptionTitle -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/CaptionTitle">
        <rdfs:label xml:lang="en-us">Caption title</rdfs:label>
        <rdfs:subClassOf rdf:resource="&madsrdf;Title"/>
        <rdfs:comment xml:lang="en-us">Caption title, printed at the head of the first page of text.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Cartography -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Cartography">
        <rdfs:label xml:lang="en-us">Cartography</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:comment xml:lang="en-us">Resource that show spatial information, including maps, atlases, globes,digital maps, and other cartographic items.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/CirculationStatus -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/CirculationStatus">
        <rdfs:label xml:lang="en-us">Circulation status</rdfs:label>
        <rdfs:subClassOf rdf:resource="&skos;Concept"/>
        <skos:editorialNote xml:lang="en-us">Need a vocabulary to numerate values</skos:editorialNote>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Classification -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Classification">
        <rdfs:label xml:lang="en-us">Classification</rdfs:label>
        <rdfs:subClassOf rdf:resource="&skos;Concept"/>
        <rdfs:comment xml:lang="en-us">System of coding, assorting and organizing materials. It may be according to their subject or other category (e.g. Author Lastname, Specimen name, in the case of art it could be by country of origin or style period).</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Coden -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Coden">
        <rdfs:label xml:lang="en-us">CODEN</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">Identifier for scientific and technical periodical titles assigned by the International CODEN Section of Chemical Abstracts Service.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Collection -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Collection">
        <rdfs:label xml:lang="en-us">Collection</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:comment xml:lang="en-us">Aggregation of resources, generally gathered together artificially. A Collection is generally gathered at the receiving end, while a Series is gathered together at the publishing end.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/ConciseTitle -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/ConciseTitle">
        <rdfs:label xml:lang="en-us">Concise title</rdfs:label>
        <rdfs:subClassOf rdf:resource="&madsrdf;Title"/>
        <skos:example>&apos;Modern Writing&apos; could serve as the concise title for &apos;The Berkley Book of Modern Writing&apos;.</skos:example>
        <rdfs:comment xml:lang="en-us">The concise version of a Title.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/ContainerTitle -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/ContainerTitle">
        <rdfs:label>Container title</rdfs:label>
        <rdfs:subClassOf rdf:resource="&madsrdf;Title"/>
        <rdfs:comment>Title on a case or container, such as a CD case.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/ContinuingResource -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/ContinuingResource">
        <rdfs:label>Continuing resource</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <owl:disjointWith rdf:resource="http://ld4l.org/ontology/bib/FiniteResource"/>
        <skos:example>The New York Times (newspaper)</skos:example>
        <rdfs:comment>A resource that is conceived and planned to be ongoing and have no defined endpoint.</rdfs:comment>
        <skos:scopeNote>A ContinuingResource may have an actual end due to unplanned circumstances, but the intent at the time of creation was for it to be ongoing.</skos:scopeNote>
        <skos:scopeNote>Constrasts with FiniteResource.</skos:scopeNote>
        <skos:example>Linguistic Inquiry (academic journal)</skos:example>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Contribution -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Contribution">
        <rdfs:label xml:lang="en">Contribution</rdfs:label>
        <rdfs:comment xml:lang="en">A contribution to a work by an agent.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/CoverTitle -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/CoverTitle">
        <rdfs:label xml:lang="en-us">Cover title</rdfs:label>
        <rdfs:subClassOf rdf:resource="&madsrdf;Title"/>
        <rdfs:comment xml:lang="en-us">Cover title that is printed on the original cover of a publication or lettered or stamped on the publisher&apos;s binding.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Dataset -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Dataset">
        <rdfs:label xml:lang="en-us">Dataset</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:comment xml:lang="en-us">Data encoded in a defined structure. Includes numeric data, environmental data,etc., used by applications software to calculate averages, correlations, etc., or to produce models, etc., but not normally displayed in its raw form.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/DdcClassification -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/DdcClassification">
        <rdfs:label>DDC classification</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Classification"/>
        <rdfs:comment>Dewey Decimal Classification number used for subject access.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Dissertation -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Dissertation">
        <rdfs:label xml:lang="en-us">Dissertation</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/FiniteResource"/>
        <rdfs:comment xml:lang="en-us">A thesis or disseration written in association with earning an academic degree.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/DissertationIdentifier -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/DissertationIdentifier">
        <rdfs:label xml:lang="en-us">Dissertation identifier</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">A local identifier assigned to a dissertation written at that institution.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/DistinctiveTitle -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/DistinctiveTitle">
        <rdfs:label xml:lang="en-us">Distinctive title</rdfs:label>
        <rdfs:subClassOf rdf:resource="&madsrdf;Title"/>
        <rdfs:comment xml:lang="en-us">Special title that appears in addition to the regular title on individual issues of a work and by which the issue may be known.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Doi -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Doi">
        <rdfs:label xml:lang="en-us">DOI</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">Digital Object Identifier.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Ean -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Ean">
        <rdfs:label xml:lang="en-us">EAN</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">International Article Number.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Electronic -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Electronic">
        <rdfs:label xml:lang="en-us">Electronic</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Instance"/>
        <rdfs:comment xml:lang="en-us">Resource that is intended for manipulation by a computer, residing in a carrier accessed either directly or remotely.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Family -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Family">
        <rdfs:label xml:lang="en-us">Family</rdfs:label>
        <rdfs:subClassOf rdf:resource="&foaf;Agent"/>
        <rdfs:comment xml:lang="en-us">Two or more persons related by birth, marriage, adoption, civil union, or similar legal status, or who otherwise present themselves as a family.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Fingerprint -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Fingerprint">
        <rdfs:label xml:lang="en-us">Fingerprint</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">Identifier that is used to assist in the identification of antiquarian books by recording information comprising groups of characters taken from specified positions on specified pages of the book.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/FiniteResource -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/FiniteResource">
        <rdfs:label>Finite resource</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:comment>A resource that is conceived and planned to have a defined endpoint. It may consist of a single part or multiple parts (e.g., a MultipartMonograph).</rdfs:comment>
        <skos:example>Pride and Prejudice</skos:example>
        <skos:example>The Lord of the Rings trilogy</skos:example>
        <skos:scopeNote>Constrasts with a ContinuingResource, which is conceived and planned to be ongoing with no defined endpoint.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Hdl -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Hdl">
        <rdfs:label xml:lang="en-us">HDL</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">Unique and persistent identifier for digital objects developed by the Corporation for National Research Initiatives.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Identifier -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Identifier">
        <rdfs:label xml:lang="en-us">Identifier</rdfs:label>
        <rdfs:comment xml:lang="en-us">Token or name that is associated with a resource, such as a URI or an ISBN.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Index -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Index">
        <rdfs:label xml:lang="en-us">Index</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:comment xml:lang="en-us">The index of a resource.</rdfs:comment>
        <skos:scopeNote xml:lang="en-us">Use dcterms:isPartOf to relate Index to Work it is a part of.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Instance -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Instance">
        <rdfs:label xml:lang="en-us">Instance</rdfs:label>
        <owl:disjointWith rdf:resource="http://ld4l.org/ontology/bib/Item"/>
        <owl:disjointWith rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:comment xml:lang="en-us">Resource reflecting an individual, material embodiment of the Work.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/IntegratingResource -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/IntegratingResource">
        <rdfs:label xml:lang="en-us">Integrating resource</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <owl:disjointWith rdf:resource="http://ld4l.org/ontology/bib/Monograph"/>
        <owl:disjointWith rdf:resource="http://ld4l.org/ontology/bib/Serial"/>
        <skos:example>A website that is updated continuously.</skos:example>
        <skos:example>A loose-leaf manual that is updated by means of replacement pages.</skos:example>
        <skos:example>Final print edition of AACR2 published in a binder.</skos:example>
        <skos:scopeNote>An IntegratingResource may be either finite (having a predetermined conclusion) or continuous. The class itself is therefore not a subclass of FiniteResource or ContinuingResource, but a specific member would belong to one of these two classes.</skos:scopeNote>
        <rdfs:comment xml:lang="en-us">Cataloging resource that is added to or changed by means of updates that do not remain discrete but are integrated into the whole.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Isan -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Isan">
        <rdfs:label xml:lang="en-us">ISAN</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">International Standard Audiovisual Number.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Isbn -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Isbn">
        <rdfs:label xml:lang="en-us">ISBN</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">International Standard Book Number.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Isbn-10 -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Isbn-10">
        <rdfs:label xml:lang="en-us">ISBN-10</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Isbn"/>
        <rdfs:comment xml:lang="en-us">10 digit version of the International Standard Book Number.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Isbn-13 -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Isbn-13">
        <rdfs:label xml:lang="en-us">ISBN-13</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Isbn"/>
        <rdfs:comment xml:lang="en-us">13 digit version of the International Standard Book Number.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Ismn -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Ismn">
        <rdfs:label xml:lang="en-us">ISMN</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">International Standard Music Number.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Iso -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Iso">
        <rdfs:label xml:lang="en-us">ISO</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">International Organization for Standardization standard number.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Issn -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Issn">
        <rdfs:label xml:lang="en-us">ISSN</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">International Standard Serial Number identifier.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Issn-L -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Issn-L">
        <rdfs:label xml:lang="en-us">ISSN-L</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">International Standard Serial Number that links together various media versions of a continuing resource.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/IssueNumber -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/IssueNumber">
        <rdfs:label xml:lang="en-us">Issue number</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">Number used to identify the issue designation, or serial identification, assigned by a publisher to a sound recording.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Istc -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Istc">
        <rdfs:label xml:lang="en-us">ISTC</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">International Standard Text code, a numbering system developed to enable the unique identification of textual works.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Iswc -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Iswc">
        <rdfs:label xml:lang="en-us">ISWC</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">International Standard Musical Work Code, a unique, persistent reference number for the identification of musical works.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Item -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Item">
        <rdfs:label xml:lang="en-us">Item</rdfs:label>
        <owl:disjointWith rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:comment xml:lang="en-us">A singular physical item that can be held, circulated, etc. Equivalant to the FRBR Item class.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/KeyTitle -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/KeyTitle">
        <rdfs:label xml:lang="en-us">Key title</rdfs:label>
        <rdfs:subClassOf rdf:resource="&madsrdf;Title"/>
        <rdfs:comment xml:lang="en-us">Unique title for a continuing resource that is assigned by the ISSN International Center in conjunction with an ISSN.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/LcOverseasAcqNumber -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/LcOverseasAcqNumber">
        <rdfs:label xml:lang="en-us">LC overseas acq number</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">Identification number assigned by the Library of Congress to works acquired through one of its overseas acquisition programs.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/LccClassification -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/LccClassification">
        <rdfs:label>LCC classification</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Classification"/>
        <rdfs:comment>Library of Congress Classification number used for subject access.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Lccn -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Lccn">
        <rdfs:label xml:lang="en-us">LCCN</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">Library of Congress Control Number, which identifies the resource description.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/LegalDeposit -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/LegalDeposit">
        <rdfs:label xml:lang="en-us">Legal deposit</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">Number assigned to a copyright or legal deposit, which Identifies a resource description.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Manuscript -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Manuscript">
        <rdfs:label xml:lang="en-us">Manuscript</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Item"/>
        <rdfs:comment xml:lang="en-us">Resource that is written in handwriting or typescript. These are generally unique resources.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/MatrixNumber -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/MatrixNumber">
        <rdfs:label xml:lang="en-us">Matrix number</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">Matrix number from the master from which a specific sound recording was pressed.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Monograph -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Monograph">
        <rdfs:label xml:lang="en-us">Monograph</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/FiniteResource"/>
        <owl:disjointWith rdf:resource="http://ld4l.org/ontology/bib/Serial"/>
        <rdfs:comment xml:lang="en-us">A resource that is complete in one part or intended to be complete within a finite number of parts.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/MovingImage -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/MovingImage">
        <rdfs:label xml:lang="en-us">Moving image</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:comment xml:lang="en-us">Images intended to be perceived as moving, including motion pictures (using liveaction and/or animation); film and video recordings of performances and events; etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Multimedia -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Multimedia">
        <rdfs:label xml:lang="en-us">Multimedia</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:comment xml:lang="en-us">Electronic resource that is a computer program (i.e. digitally encoded instructions intended to be processed and performed by a computer) or which consists of multiple media types that are software driven.</rdfs:comment>
        <skos:example>Examples include video games and websites.</skos:example>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/MultipartMonograph -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/MultipartMonograph">
        <rdfs:label xml:lang="en-us">Multipart monograph</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Monograph"/>
        <skos:example>A dictionary in two volumes.</skos:example>
        <skos:example>Three audiocassettes issued as a set.</skos:example>
        <skos:example>The television series Cosmos is a MultipartMonograph (having a defined endpoint) with no continuous narrative.</skos:example>
        <rdfs:comment xml:lang="en-us">A resource issued in two or more parts (either simultaneously or successively) that is complete or intended to be completed within a finite number of parts.</rdfs:comment>
        <skos:example>The Lord of the Rings is a MultipartMonograph with a continuous narrative.</skos:example>
        <skos:scopeNote>A MultipartMonograph often has a continuous narrative. The component Monographs can then be linked using the predicates ld4l:precedesInNarrative and ld4l:succeedsInNarrative, especially if there is no PartNameElement of the Title to indicate sequencing.</skos:scopeNote>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/MusicPlate -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/MusicPlate">
        <rdfs:label xml:lang="en-us">Music plate</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">Number assigned by a music publisher to a specific music publication.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/MusicPublisherNumber -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/MusicPublisherNumber">
        <rdfs:label xml:lang="en-us">Music publisher number</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">Number assigned to a music publication other than an issue, matrix, or plate number</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Nban -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Nban">
        <rdfs:label xml:lang="en-us">NBAN</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">National Bibliography Agency Number, which identifies the resource description.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Nbn -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Nbn">
        <rdfs:label xml:lang="en-us">NBN</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">National Bibliography Number, which identifies the resource description.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/NlmClassification -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/NlmClassification">
        <rdfs:label>NLM classification</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Classification"/>
        <rdfs:comment>National Library of Medicine Classification number used for subject access.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/NotatedMovement -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/NotatedMovement">
        <rdfs:label xml:lang="en-us">Notated movement</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:comment xml:lang="en-us">Graphic, non-realized representation of movement intended to be perceived visually, e.g. dance.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/NotatedMusic -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/NotatedMusic">
        <rdfs:label xml:lang="en-us">Notated music</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Work"/>
        <rdfs:comment xml:lang="en-us">Graphic, non-realized representation of a musical work intended to be perceived visually.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Periodical -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Periodical">
        <rdfs:label>Periodical</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Serial"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dcterms;hasPart"/>
                <owl:allValuesFrom rdf:resource="http://ld4l.org/ontology/bib/PeriodicalPart"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:scopeNote>A Periodical is related to its parts via the predicate dcterms:hasPart.</skos:scopeNote>
        <skos:scopeNote>In a Periodical, as opposed to a Series (the other main subclass of Serial), the Periodical itself is the primary focus and has the primary identity; the parts are not stand-alone works.</skos:scopeNote>
        <skos:example>The New Yorker (magazine)</skos:example>
        <skos:example>Linguistic Inquiry (academic journal)</skos:example>
        <skos:editorialNote>Possibly we can assert the disjointness of Series and Periodical, but there may be edge cases that are categorized as both.</skos:editorialNote>
        <skos:scopeNote>A Periodical will typically have a ld4l:frequency value to identify its intended periodicity. While it&apos;s true that intent can be subjective, there are usually clear indications, such as a stated frequency of &quot;monthly.&quot;</skos:scopeNote>
        <rdfs:comment>A continuing resource, typically issued at regular intervals, in which the identity of the whole predominates over the identity of the parts. The parts usually look alike, being distinguished from one another by number and date designations, though they may also have individual titles. </rdfs:comment>
        <skos:example>The New York Times (newspaper)</skos:example>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/PeriodicalPart -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/PeriodicalPart">
        <rdfs:label xml:lang="en-us">Periodical part</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/FiniteResource"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dcterms;isPartOf"/>
                <owl:allValuesFrom rdf:resource="http://ld4l.org/ontology/bib/Periodical"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dcterms;hasPart"/>
                <owl:allValuesFrom rdf:resource="http://ld4l.org/ontology/bib/PeriodicalPart"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="&dcterms;isPartOf"/>
                <owl:minCardinality rdf:datatype="&xsd;nonNegativeInteger">1</owl:minCardinality>
            </owl:Restriction>
        </rdfs:subClassOf>
        <skos:example>Volume 3</skos:example>
        <skos:editorialNote>We may want to define subclasses Volume, Issue, Part, etc. if these have distinct properties. Otherwise, they can simply be identified by their rdfs:labels.</skos:editorialNote>
        <skos:example>The June 1, 2015 issue of The New Yorker</skos:example>
        <skos:example>July 31, 2015</skos:example>
        <skos:example>Issue 12</skos:example>
        <skos:scopeNote>A PeriodicalPart is related to the enclosing Periodical via dcterms:isPartOf and inverse dcterms:hasPart. If a PeriodicalPart contains other PeriodicalParts, such as a Periodical volume that contains issues, these are also related using dcterms:hasPart.</skos:scopeNote>
        <rdfs:comment xml:lang="en-us">One unit or member of a Periodical. Can be a volume, issue, part, etc.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Policy -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Policy">
        <rdfs:label xml:lang="en-us">Policy</rdfs:label>
        <rdfs:comment xml:lang="en-us">Document specifying a single policy.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/PolicySet -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/PolicySet">
        <rdfs:label xml:lang="en-us">Policy set</rdfs:label>
        <rdfs:subClassOf rdf:resource="&skos;Collection"/>
        <rdfs:comment>A set of policies governing one or more bibliographic resources.</rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/PostalRegistrationNumber -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/PostalRegistrationNumber">
        <rdfs:label xml:lang="en-us">Postal registration number</rdfs:label>
        <rdfs:subClassOf rdf:resource="http://ld4l.org/ontology/bib/Identifier"/>
        <rdfs:comment xml:lang="en-us">Number assigned to a publication for which the specified postal service permits the use of a special mailing class privilege. </rdfs:comment>
    </owl:Class>
    


    <!-- http://ld4l.org/ontology/bib/Print -->

    <owl:Class rdf:about="http://ld4l.org/ontology/bib/Print">

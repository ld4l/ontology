<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE rdf:RDF [
    <!ENTITY bf "http://bibframe.org/vocab/" >
    <!ENTITY owl "http://www.w3.org/2002/07/owl#" >
    <!ENTITY xsd "http://www.w3.org/2001/XMLSchema#" >
    <!ENTITY rdfs "http://www.w3.org/2000/01/rdf-schema#" >
    <!ENTITY rdf "http://www.w3.org/1999/02/22-rdf-syntax-ns#" >
    <!ENTITY bflegacy "http://bibframe.org/vocab/legacy/" >    
]>


<rdf:RDF 
    xmlns:xsd="http://www.w3.org/2001/XMLSchema#" 
    xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#" 
    xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#" 
    xmlns:dcterms="http://purl.org/dc/terms/" 
    xmlns:owl="http://www.w3.org/2002/07/owl#"  
    xmlns:bf="http://bibframe.org/vocab/"
    xml:base="http://bibframe.org/vocab/legacy/">

    <owl:Ontology rdf:about="">
        <rdfs:label>MARC Legacy Terms</rdfs:label>
        <dcterms:title>MARC Legacy Terms</dcterms:title>
        <dcterms:description>Legacy terms used to preserve MARC data in its existing form. Data will also be remodeled in the primary bibframe ontology, where possible.</dcterms:description>
    </owl:Ontology>
    
    <!-- http://bibframe.org/vocab/legacy/notation -->
    
    <owl:AnnotationProperty rdf:about="notation">
        <rdfs:label>Notation system</rdfs:label>
        <rdfs:comment>Information on the alphabet, script, or symbol system used to convey the content of the resource, including specialized scripts, typefaces, tactile notation, and musical notation.</rdfs:comment>
        <rdfs:domain rdf:resource="&bf;Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:AnnotationProperty>

    <!-- http://bibframe.org/vocab/legacy/geographicCoverageNote -->
    
    <owl:AnnotationProperty rdf:about="geographicCoverageNote">
        <rdfs:label>Geographic coverage</rdfs:label>
        <rdfs:comment>Geographic entities covered by the resource.</rdfs:comment>
        <rdfs:domain rdf:resource="&bf;Work"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:AnnotationProperty>

    
    <!-- http://bibframe.org/vocab/legacy/musicMediumNote -->
    
    <owl:DatatypeProperty rdf:about="musicMediumNote">
        <rdfs:label>Music medium note</rdfs:label>
        <rdfs:comment>Instrumental, vocal, and/or other medium of performance for which a musical resource was originally conceived, written or performed.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>


    <!-- http://bibframe.org/vocab/legacy/duration -->
    
    <owl:DatatypeProperty rdf:about="duration">
        <rdfs:label>Duration</rdfs:label>
        <rdfs:comment>Information about the playing time or duration in an unstructured form, e.g., &quot;2 hours&quot;.</rdfs:comment>
        <rdfs:domain rdf:resource="&bf;Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    
    <!-- http://bibframe.org/vocab/legacy/graphicScaleNote -->
    
    <owl:DatatypeProperty rdf:about="graphicScaleNote">
        <rdfs:label>Graphic scale</rdfs:label>
        <rdfs:comment>Textual information about scale, including the scale of graphic material item such as architectural drawings or three-dimensional artifacts.</rdfs:comment>
        <rdfs:domain rdf:resource="&bf;Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>


    <!-- http://bibframe.org/vocab/legacy/providerStatement -->
    
    <owl:DatatypeProperty rdf:about="providerStatement">
        <rdfs:label>Provider statement</rdfs:label>
        <rdfs:comment>Transcribed provider statement</rdfs:comment>
        <rdfs:domain rdf:resource="&bf;Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>

    <!-- http://bibframe.org/vocab/legacy/supplementaryContentNote -->
    
    <owl:DatatypeProperty rdf:about="supplementaryContentNote">
        <rdfs:label>Supplementary content note</rdfs:label>
        <rdfs:comment>Information on the presence of one or more bibliographies, discographies, filmographies, and/or other bibliographic references in a described resource or in accompanying material.</rdfs:comment>
        <rdfs:domain rdf:resource="&bf;Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>


    <!-- http://bibframe.org/vocab/legacy/modeOfIssuance -->
    
    <owl:AnnotationProperty rdf:about="modeOfIssuance">
        <rdfs:label>Mode of issuance</rdfs:label>
        <rdfs:comment>Categorization reflecting whether a resource is issued in one or more parts, the way it is updated, and its intended termination.</rdfs:comment>
        <rdfs:domain rdf:resource="&bf;Instance"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>        
    </owl:AnnotationProperty>
    
    
    
    <!-- http://bibframe.org/vocab/legacy/musicKey -->
    
    <owl:DatatypeProperty rdf:about="musicKey">
        <rdfs:label>Music Key</rdfs:label>
        <rdfs:comment>Pitch and mode for music.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>



    <!-- http://bibframe.org/vocab/legacy/languageNote -->
    
    <owl:DatatypeProperty rdf:about="languageNote">
        <rdfs:label>Language note</rdfs:label>
        <rdfs:comment>Note concerning the language of the material or its parts.</rdfs:comment>
        <rdfs:domain rdf:resource="&bf;Work"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>
    
    

    <!-- http://bibframe.org/vocab/legacy/findingAidNote -->
    
    <owl:DatatypeProperty rdf:about="findingAidNote">
        <rdfs:label>Index or finding aid note</rdfs:label>
        <rdfs:comment>Note about availability of an index or finding aid.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:subClassOf rdf:resource="&bf;note"/>
    </owl:DatatypeProperty>

    <!-- http://bibframe.org/vocab/legacy/legalDate -->
    
    <owl:DatatypeProperty rdf:about="legalDate">
        <rdfs:label>Date of legal work</rdfs:label>
        <rdfs:comment>Date of legal work, or promulgation of a law, or signing of a treaty.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
    </owl:DatatypeProperty>




    <!-- http://bibframe.org/vocab/legacy/creditsNote -->
    
    <owl:DatatypeProperty rdf:about="creditsNote">
        <rdfs:label>Credits note</rdfs:label>
        <rdfs:comment>Credits for persons or organizations, other than members of the cast, who have participated in the creation and/or production of the.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:subClassOf rdf:resource="&bf;note"/>
    </owl:DatatypeProperty>
    

    <!-- http://bibframe.org/vocab/legacy/temporalCoverageNote -->
    
    <owl:DatatypeProperty rdf:about="temporalCoverageNote">
        <rdfs:label>Temporal coverage</rdfs:label>
        <rdfs:comment>Time period covered by the resource.</rdfs:comment>
        <rdfs:domain rdf:resource="&bf;Work"/>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:subClassOf rdf:resource="&bf;note"/>
    </owl:DatatypeProperty>

    <!-- http://bibframe.org/vocab/legacy/performerNote -->
    
    <owl:DatatypeProperty rdf:about="performerNote">
        <rdfs:label>Performer note</rdfs:label>
        <rdfs:comment>Information about the participants, players, narrators, presenters, or performers.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:subClassOf rdf:resource="&bf;note"/>
    </owl:DatatypeProperty>


    <!-- http://bibframe.org/vocab/legacy/treatySignator -->
    
    <owl:DatatypeProperty rdf:about="treatySignator">
        <rdfs:label>Signatory to a treaty</rdfs:label>
        <rdfs:comment>Government of other party that has formally signed a treaty.</rdfs:comment>
        <rdfs:range rdf:resource="&rdfs;Literal"/>
        <rdfs:subClassOf rdf:resource="&bf;note"/>
    </owl:DatatypeProperty>
    
</rdf:RDF>